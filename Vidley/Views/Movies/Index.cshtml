@model IEnumerable<Vidley.Models.Movie>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies (Index)</h2>
@Html.ActionLink("Create","New", "Movies")
<ul>Movies</ul>
@foreach(var movie in Model)
{
    @*<li>@movie.Name</li>*@
    <li>@Html.ActionLink(movie.Name, "Edit", "Movies")</li>
}

